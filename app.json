{
  "expo": {
    "name": "MDM",
    "slug": "MDM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/emamilogo1.jpeg",
    "scheme": "amsdev",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aonDigicon.MDM"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/emamilogo1.jpeg",
        "backgroundImage": "./assets/images/emamilogo1.jpeg",
        "monochromeImage": "./assets/images/emamilogo1.jpeg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.aonDigicon.MDM"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/emamilogo1.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/emamilogo1.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a3413b67-3398-46f8-9c25-03f752a69307"
      }
    }
  }
}
